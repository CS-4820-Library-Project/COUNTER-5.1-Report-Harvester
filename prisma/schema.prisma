generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./search.db"
}

model Report {
  id               Int            @id @default(autoincrement())
  report_id        String
  report_name      String
  release          String
  institution_name String
  institution_id   String
  created          String
  created_by       String
  ReportFilter     ReportFilter[]

  // PR_Item
  PR_Item PR_Item[]

  // PR_P1_Item
  PR_P1_Item PR_P1_Item[]

  // DR_Item
  DR_Item DR_Item[]

  // DR_D1_Item
  DR_D1_Item DR_D1_Item[]

  // DR_D2_Item
  DR_D2_Item DR_D2_Item[]

  // TR_Item
  TR_Item TR_Item[]

  // TR_B1_Item
  TR_B1_Item TR_B1_Item[]

  // TR_B2_Item
  TR_B2_Item TR_B2_Item[]

  // TR_B3_Item
  TR_B3_Item TR_B3_Item[]

  // TR_J1_Item
  TR_J1_Item TR_J1_Item[]

  // TR_J2_Item
  TR_J2_Item TR_J2_Item[]

  // TR_J3_Item
  TR_J3_Item TR_J3_Item[]

  // TR_J4_Item
  TR_J4_Item TR_J4_Item[]

  // IR_Item
  IR_Item IR_Item[]

  // IR_A1_Item
  IR_A1_Item IR_A1_Item[]

  // IR_M1_Item
  IR_M1_Item IR_M1_Item[]
}

model ReportFilter {
  id          Int    @id @default(autoincrement())
  reportId    Int
  report      Report @relation(fields: [reportId], references: [id])
  filter_type String
  value       String
}

// PR_Item model
model PR_Item {
  id            Int             @id @default(autoincrement())
  reportId      Int
  report        Report          @relation(fields: [reportId], references: [id])
  platform      String
  metricType    String
  periodTotal   Int
  PR_ItemMetric PR_ItemMetric[]
}

model PR_ItemMetric {
  id           Int     @id @default(autoincrement())
  reportItemId Int
  period       String
  value        Int
  metricType   String
  PR_Item      PR_Item @relation(fields: [reportItemId], references: [id])
}

